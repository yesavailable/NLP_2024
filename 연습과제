        "import re\n",
        "\n",
        "e=\"some,someone,somewhere,something\"\n",
        "pattern=r\"some?\"\n",
        "matches=re.findall(pattern,e)\n",
        "\n",
        "for i in matches:\n",
        "  print(\"Match:\",i)"
